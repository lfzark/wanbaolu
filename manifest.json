{
  "name": "万宝录",
  "description" : "GitHub Star管理",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
          "storage"
        ],
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js":[
        "index.js"
      ],
      "css": [
        "index.css"
      ]
    }
  ]
}
